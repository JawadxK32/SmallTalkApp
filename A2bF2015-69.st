Object subclass: #HippoStack	instanceVariableNames: 'values'	classVariableNames: ''	poolDictionaries: ''	category: 'A2bF2015-#69'!!HippoStack methodsFor: 'all' stamp: 'PietroAndreoli 10/2/2015 14:37'!initialize	values := OrderedCollection new.! !!HippoStack methodsFor: 'all' stamp: 'PietroAndreoli 10/6/2015 14:47'!values	^values! !!HippoStack methodsFor: 'all' stamp: 'PietroAndreoli 10/6/2015 14:50'!push: anObject 	self values addFirst: anObject.! !!HippoStack methodsFor: 'all' stamp: 'PietroAndreoli 10/7/2015 10:15'!print	[values size == 0] whileFalse:[ 		self pop print.	 ]! !!HippoStack methodsFor: 'all' stamp: 'PietroAndreoli 10/7/2015 10:26'!printSpecial	values size timesRepeat:[		self pop printSpecial .	 ]! !!HippoStack methodsFor: 'all' stamp: 'PietroAndreoli 10/7/2015 10:21'!pop	^values removeFirst ! !Object subclass: #Hippopotamus	instanceVariableNames: 'name weight'	classVariableNames: ''	poolDictionaries: ''	category: 'A2bF2015-#69'!!Hippopotamus methodsFor: 'all' stamp: 'PietroAndreoli 10/6/2015 14:08'!weight: aWeight	weight:=aWeight! !!Hippopotamus methodsFor: 'all' stamp: 'PietroAndreoli 10/6/2015 14:09'!weight	^weight! !!Hippopotamus methodsFor: 'all' stamp: 'PietroAndreoli 10/7/2015 10:23'!print	Transcript show: 'Name and Weight: '.	Transcript show: Character tab.	Transcript show: self name.	Transcript show: Character tab.	Transcript show: weight.	Transcript cr.! !!Hippopotamus methodsFor: 'all' stamp: 'PietroAndreoli 10/6/2015 14:06'!name: aName	name := aName.	! !!Hippopotamus methodsFor: 'all' stamp: 'PietroAndreoli 10/6/2015 14:07'!name	^name! !!Hippopotamus methodsFor: 'all' stamp: 'PietroAndreoli 10/7/2015 10:26'!printSpecial		weight<500 ifTrue:[			Transcript show: 'Name and Weight under 500:'.			Transcript show: Character tab.			Transcript show: self weight.			Transcript show: Character tab.			Transcript show: name.			Transcript cr.		]! !